<!DOCTYPE html>
<html>
<head>
  <title>AgWeb</title>
  <%= stylesheet_link_tag :all %>
  <% #=  javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <% if signed_in? %>
    <%=  current_user.email %> 
    <%= link_to "Logout", signout_path, :method => :delete %>
  <% end %>
  <div id="wrapper" >
    <%= render 'layouts/header' %> 
    <%= render 'layouts/navmenu' %> 
      <div class="span-15">
      <% flash.each do |key, value| %>
        <div class="flash <%= key %> "><%= value %></div>
      <% end %>         
      <%= yield %>
    </div>        
  </div>  
</body>
</html>
