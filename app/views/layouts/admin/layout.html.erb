<!DOCTYPE html>
<html>
<head>
  <title><%= admin_title %></title>
  <%= stylesheet_link_tag 'screen', 'ie', 'custom' %>
  <%=  javascript_include_tag :defaults, "rails.validations"  %>
  <%= csrf_meta_tag %>
</head>
<body>  
  <div id="wrapper" >
    <%= render 'layouts/admin/header' %> 
    <%= render 'layouts/admin/navmenu' %> 
    <div id="contenent" class="span-13">
      <% flash.each do |key, value| %>
        <div class="flash <%= key %> "><%= value %></div>
      <% end %>         
      <%= yield %>
    </div>    
    <br clear="all"/>
  </div> 
    <br clear="all"/>
    <%= render 'layouts/admin/footer' %>
    
  
</body>
</html>
