<h1>Listing Users</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Mail</th>
    <th></th>
    <th>Abilitato</th>
    <th>Admin</th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td></td>
    <td><%= user.enabled %></td>
    <td><%= user.admin %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>

